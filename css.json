	"grid": {
		"prefix": "gd",
		"body": [
		  "display: grid;",
		  "      grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); /* Ensure the columns adjust dynamically */",
		  "      grid-gap: 10px;",
		  "      justify-content: center; /* Center the grid items horizontally */",
		  "      align-items: center; /* Center the grid items vertically */",
		  "      margin: 0 auto; /* Center the grid horizontally */",
		  "      max-width: 1200px; /* Limit the maximum width of the grid */",
		  "	  "
		],
		"description": "grid"
	  },
	"5-Row CSS Grid Layout": {
		"scope": "css",
		"prefix": "grid",
		"body": [
			".${1:grid}-container {",
			"\tdisplay: grid;",
			"\tgrid-template-rows: repeat(5, 1fr);",
			"\tgrid-gap: ${2:10}px;",
			"\t${3:// Additional grid properties here}",
			"}",
			"",
			".${1:grid} {",
			"\tgrid-row: span 1;",
			"\t${4:// Additional item properties here}",
			"}"
		],
		"description": "5-row CSS grid layout"
	},
