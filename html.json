		"Jinja If Statement": {
		  "scope": "html",
		  "prefix": "if",
		  "body": [
			"{% if ${1:condition} %}",
			"\t${2:content}",
			"{% endif %}"
		  ],
		  "description": "Jinja if statement"
		},
	  
		"Jinja For Loop": {
		  "scope": "html",
		  "prefix": "for",
		  "body": [
			"{% for ${1:item} in ${2:items} %}",
			"\t${3:{{ item }}",
			"{% endfor %}"
		  ],
		  "description": "Jinja for loop"
		},
	  
		"Jinja Macro": {
		  "scope": "html",
		  "prefix": "macro",
		  "body": [
			"{% macro ${1:macro_name}(${2:params}) %}",
			"\t${3:content}",
			"{% endmacro %}"
		  ],
		  "description": "Jinja macro"
		},
	  
		"Jinja Block": {
		  "scope": "html",
		  "prefix": "block",
		  "body": [
			"{% block ${1:block_name} %}",
			"\t${2:content}",
			"{% endblock %}"
		  ],
		  "description": "Jinja block"
		},
	  
		"Jinja Include": {
		  "scope": "html",
		  "prefix": "include",
		  "body": [
			"{% include '${1:template_name}' %}"
		  ],
		  "description": "Jinja include"
		},
	  
		"Jinja Extends": {
		  "scope": "html",
		  "prefix": "ex",
		  "body": [
			"{% extends '${1:base}.html' %}"
		  ],
		  "description": "Jinja extends"
		},
	  
		"Jinja Filter": {
		  "scope": "html",
		  "prefix": "filter",
		  "body": [
			"{{ ${1:value} | ${2:filter_name} }}"
		  ],
		  "description": "Jinja filter"
		},



